<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Biotech -->

<!-- Maladie de la numérisation -->
	<AchievementsExpanded.AchievementDef MayRequire="Ludeon.RimWorld.Biotech">
		<defName>JD_Numerisation</defName>
		<label>Digitising the soul</label>
		<description>Use the sub-core scanner 50 times on your colonists.</description>

		<tracker Class="AchievementsExpanded.HediffTracker">
			<def>ScanningSickness</def>
			<count>50</count>
		</tracker>

		<texPath>Achievements/JD_Numerisation</texPath>
		<points>25</points>
	</AchievementsExpanded.AchievementDef>

<!-- Régénération des gènes -->
	<AchievementsExpanded.AchievementDef MayRequire="Ludeon.RimWorld.Biotech">
		<defName>JD_RegenDesGenes</defName>
		<label>On the road to perfection</label>
		<description>Collect 50 gene packs from your colonists.</description>

		<tracker Class="AchievementsExpanded.HediffTracker">
			<def>XenogermReplicating</def>
			<count>50</count>
		</tracker>

		<texPath>Achievements/JD_RegenDesGenes</texPath>
		<points>25</points>
	</AchievementsExpanded.AchievementDef>

